{
    "name": "omisai/vies-rest",
    "description": "PHP package to check Vat number through REST API of VIES",
    "keywords": [
        "vies",
        "sdk",
        "rest",
        "api"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omisai\\ViesRest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "homepage": "https://github.com/omisai-tech/php-vies-rest",
    "authors": [
        {
            "name": "SÃ¡ndor Gergely Bornemisza",
            "email": "bornemisza@omisai.com",
            "homepage": "https://omisai.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^4.3",
        "laravel/pint": "^1.27"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "scripts": {
        "test": "./vendor/bin/pest --colors=always",
        "compile": "php compiler.php",
        "pint": "./vendor/laravel/pint/builds/pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}